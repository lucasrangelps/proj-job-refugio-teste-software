@model IEnumerable<Job_refugio_bd.Models.Curriculo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FormAcad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResumoQualific)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrincRealiza)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExpProf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ObjProf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CursosComplIdioma)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Candidato)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormAcad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResumoQualific)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrincRealiza)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpProf)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObjProf)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CursosComplIdioma)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Candidato.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
